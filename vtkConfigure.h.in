/* 
 * here is where system comupted values get stored these values should only
 * change when the target compile platform changes 
 */

/* what byte order */
#cmakedefine CMAKE_WORDS_BIGENDIAN
#ifdef CMAKE_WORDS_BIGENDIAN
  #define VTK_WORDS_BIGENDIAN 
#endif

/* what threading system are we using */
#cmakedefine CMAKE_USE_PTHREADS
#ifdef CMAKE_USE_PTHREADS
#define VTK_USE_PTHREADS
#endif

#cmakedefine CMAKE_USE_SPROC
#ifdef CMAKE_USE_SPROC
#define VTK_USE_SPROC
#endif

#cmakedefine CMAKE_HP_PTHREADS
#ifdef CMAKE_HP_PTHREADS
#define VTK_HP_PTHREADS
#endif

#cmakedefine CMAKE_USE_WIN32_THREADS
#ifdef CMAKE_USE_WIN32_THREADS
#define VTK_USE_WIN32_THREADS
#endif

#cmakedefine VTK_EXPLICIT_TEMPLATES
#ifndef VTK_EXPLICIT_TEMPLATES
#define VTK_NO_EXPLICIT_TEMPLATE_INSTANTIATION
#endif

#cmakedefine CMAKE_NO_ANSI_STRING_STREAM
#ifdef CMAKE_NO_ANSI_STRING_STREAM
#define VTK_NO_ANSI_STRING_STREAM
#endif

#cmakedefine CMAKE_NO_STD_NAMESPACE
#ifdef CMAKE_NO_STD_NAMESPACE
#define VTK_NO_STD_NAMESPACE
#endif

#cmakedefine VTK_BUILD_SHARED_LIBS
#ifdef VTK_BUILD_SHARED_LIBS
#define VTKDLL
#else
#define VTKSTATIC
#endif

#cmakedefine VTK_USE_64BIT_IDS

#cmakedefine VTK_USE_ANSI_STDLIB

/* Whether VTK was built to support Carbon or Cocoa on the Mac.  */
#cmakedefine VTK_USE_CARBON
#cmakedefine VTK_USE_COCOA

#ifdef VTK_USE_CARBON
# define USE_NON_CONST
# define MAC_OSX_TK
#endif

/* compiler used */
#define VTK_CXX_COMPILER "${CMAKE_CXX_COMPILER}"

#define VTK_MAJOR_VERSION @VTK_MAJOR_VERSION@
#define VTK_MINOR_VERSION @VTK_MINOR_VERSION@
#define VTK_BUILD_VERSION @VTK_BUILD_VERSION@
#define VTK_VERSION "@VTK_VERSION@"

/* Size of fundamental data types.  */
#cmakedefine VTK_SIZEOF_CHAR   ${VTK_SIZEOF_CHAR}
#cmakedefine VTK_SIZEOF_DOUBLE ${VTK_SIZEOF_DOUBLE}
#cmakedefine VTK_SIZEOF_FLOAT  ${VTK_SIZEOF_FLOAT}
#cmakedefine VTK_SIZEOF_INT    ${VTK_SIZEOF_INT}
#cmakedefine VTK_SIZEOF_LONG   ${VTK_SIZEOF_LONG}
#cmakedefine VTK_SIZEOF_SHORT  ${VTK_SIZEOF_SHORT}

/* Whether type "bool" is supported by the compiler.  */
#cmakedefine VTK_COMPILER_HAS_BOOL
