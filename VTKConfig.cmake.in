# The names of utility libraries used by VTK.
SET(VTK_PNG_LIBRARIES      "@VTK_PNG_LIBRARIES@")
SET(VTK_ZLIB_LIBRARIES     "@VTK_ZLIB_LIBRARIES@")
SET(VTK_JPEG_LIBRARIES     "@VTK_JPEG_LIBRARIES@")
