# The VTK test script locations.
SET(VTK_HEADER_TESTING_PY "@VTK_HEADER_TESTING_PY_CONFIG@")
SET(VTK_FIND_STRING_TCL "@VTK_FIND_STRING_TCL_CONFIG@")
SET(VTK_PRINT_SELF_CHECK_TCL "@VTK_PRINT_SELF_CHECK_TCL_CONFIG@")
