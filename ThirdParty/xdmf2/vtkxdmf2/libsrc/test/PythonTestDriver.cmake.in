SET(ENV{PYTHONPATH} $ENV{PYTHONPATH}:@CMAKE_LIBRARY_OUTPUT_DIRECTORY@:@Xdmf_BINARY_DIR@/libsrc)
SET(ENV{LD_LIBRARY_PATH} $ENV{LD_LIBRARY_PATH}:@CMAKE_LIBRARY_OUTPUT_DIRECTORY@:@Xdmf_BINARY_DIR@/libsrc)

MESSAGE("CALLING: " ${ARGV})
EXECUTE_PROCESS(
   COMMAND "@PYTHON_EXECUTABLE@" ${ARGV} 
   RESULT_VARIABLE import_res
   OUTPUT_VARIABLE import_output
